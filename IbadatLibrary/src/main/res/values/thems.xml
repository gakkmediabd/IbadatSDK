<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CircularProgressBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/drawable_circle_green</item>
        <item name="colorAccent">@color/colorWhiteDisabled</item>
    </style>

    <style name="CardViewStyle" parent="CardView">
        <item name="cardCornerRadius">@dimen/small_spacing</item>
        <item name="cardElevation">5dp</item>
        <item name="android:padding">@dimen/small_spacing</item>
    </style>

    <style name="MaterialAlertDialog_rounded" parent="@style/ThemeOverlay.MaterialComponents.MaterialAlertDialog">
        <item name="shapeAppearanceOverlay">@style/DialogCorners</item>
    </style>

    <style name="DialogCorners">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">20dp</item>
    </style>
</resources>